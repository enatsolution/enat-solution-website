{
  "name": "candidate-profile-search",
  "version": "1.0.0",
  "description": "Search for candidate profiles from GitHub, Dev.to and other platforms",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "axios-cookiejar-support": "^6.0.5",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pdfjs-dist": "^5.4.394",
    "puppeteer-core": "^24.31.0",
    "serpapi": "^2.2.1",
    "session-file-store": "^1.5.0",
    "tough-cookie": "^6.0.0"
  }
}
